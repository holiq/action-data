{
  "preset": "laravel",

  "rules": {
    "types_spaces": {
      "space": "single"
    },
    "concat_space": {
      "spacing": "one"
    },
    "explicit_indirect_variable": true,
    "method_chaining_indentation": true,
    "no_blank_lines_after_class_opening": true,
    "single_quote": true,
    "single_line_empty_body": false,
    "new_with_parentheses": false
  }
}